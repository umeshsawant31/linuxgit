#!/bin/bash -x
#enter single digit number and displays that number in words


echo "Enter single digit number: "
read a

number=$(echo $a)

case $number in
        0)
	 echo "zero"
	;;
        1)
	 echo "one"
	;;
        2)
	 echo "two"
	;;
        3)
	 echo "three"
	;;
        4)
	 echo "four"
	;;
        5)
	 echo "five"
	;;
        6)
	 echo "six"
	;;
        7)
	 echo "seven"
	;;
        8)
	 echo "eight"
	;;
        9)
	 echo "nine"
	;;
esac


